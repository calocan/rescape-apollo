{
  "name": "rescape-apollo",
  "version": "0.0.45",
  "description": "Apollo Helpers for Rescape Projects",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:calocan/rescape-apollo.git"
  },
  "author": "calocan <andy@likuski.org>",
  "license": "MIT",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build:es": "BABEL_ENV=es NODE_ENV=es node $NODE_DEBUG_OPTION node_modules/.bin/rollup  -c",
    "build": "yarn run build:es",
    "prepareX": "yarn run clean && yarn run lint && yarn test && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g reactjs/redux && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:reactjs/redux gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn --new-version version patch && yarn publish",
    "publish-minor": "yarn --new-version version minor && yarn publish"
  },
  "dependencies": {
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-context": "^1.0.17",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-link-ws": "^1.0.17",
    "apollo-utilities": "^1.2.1",
    "debug": "^4.1.1",
    "fetch": "^1.1.0",
    "folktale": "^2.3.2",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "http-https": "^1.0.0",
    "node-fetch": "^2.2.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "ramda-lens": "^0.1.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-async-component": "^2.0.0",
    "react-dom": "^16.8.6",
    "react-emotion": "^10.0.0",
    "request": "^2.88.0",
    "rescape-geospatial-sankey": "^0.7.17",
    "rescape-helpers": "^1.1.79",
    "rescape-helpers-component": "^1.5.35",
    "rescape-ramda": "^1.5.27",
    "rescape-validate": "^1.2.15",
    "source-map-support": "^0.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@types/jest": "^24.0.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^5.2.0",
    "dotenv": "7.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-wait": "^1.0.9",
    "eslint": "5.16.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "jest": "24.7.1",
    "jest-cli": "^24.7.1",
    "jsdoc-babel": "^0.5.0",
    "localstorage-polyfill": "^1.0.1",
    "longjohn": "^0.2.12",
    "moment": "^2.24.0",
    "promise": "8.0.3",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "rescape-helpers-test": "^0.0.13",
    "rescape-sample-data": "^1.2.51",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "peerDependencies": {
    "graphql": "^14.0.2"
  },
  "jest": {
    "modulePaths": [],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/config/jestsetup.js"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
