{
  "name": "rescape-apollo",
  "version": "0.1.245",
  "description": "Apollo Helpers for Rescape Projects",
  "main": "./lib/index.js",
  "exports": "./src/index.js",
  "module": "./src/index.mjs",
  "repository": {
    "type": "git",
    "url": "git@github.com:calocan/rescape-apollo.git"
  },
  "author": "calocan <andy@likuski.org>",
  "license": "MIT",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib esm coverage",
    "lint": "eslint src",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build": "rollup -c",
    "prepare": "cross-env ENABLE_INTEGRATION_TESTS=false yarn run clean && yarn run lint && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g reactjs/redux && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:reactjs/redux gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn version --patch && yarn publish --non-interactive",
    "publish-minor": "yarn version --minor && yarn publish --non-interactive"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@apollo/link-context": "^2.0.0-beta.3",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/link-ws": "^2.0.0-beta.3",
    "@apollo/react-hooks": "^4.0.0",
    "@jest/globals": "^26.6.2",
    "apollo-cache-persist": "^0.1.1",
    "apollo-utilities": "^1.3.2",
    "debug": "^4.2.0",
    "fetch": "^1.1.0",
    "folktale": "^2.3.2",
    "graphql-tag": "^2.10.1",
    "http-https": "^1.0.0",
    "inflected": "^2.1.0",
    "jest-enzyme": "^7.1.2",
    "jsdom-global": "^3.0.2",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "pluralize": "^8.0.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "ramda-lens": "^0.1.2",
    "react": "^17.0.1",
    "react-adopt": "^0.6.0",
    "react-apollo": "^3.0.1",
    "react-async-component": "^2.0.0",
    "react-dom": "^17.0.1",
    "react-emotion": "^10.0.0",
    "regenerator-runtime": "^0.13.3",
    "request": "^2.88.0",
    "safe-json-stringify": "^1.2.0",
    "unfetch": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@types/jest": "^26.0.15",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^7.0.2",
    "dotenv": "8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-wait": "^1.0.9",
    "eslint": "7.13.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "6.2.0",
    "fs-extra": "9.0.1",
    "install-peers-cli": "^2.1.1",
    "jest": "26.6.3",
    "jest-cli": "^26.6.3",
    "jsdoc-babel": "^0.5.0",
    "jsdom": "^16.4.0",
    "localstorage-polyfill": "^1.0.1",
    "longjohn": "^0.2.12",
    "moment": "^2.29.1",
    "promise": "8.1.0",
    "rescape-sample-data": "^1.2.60",
    "rimraf": "^3.0.0",
    "rollup": "^2.33.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "graphql": ">=14.6.0",
    "rescape-geospatial-sankey": ">=0.7.17",
    "rescape-helpers": ">=1.1.99",
    "rescape-helpers-component": ">=1.5.38",
    "rescape-log": ">=0.1.11",
    "rescape-ramda": ">=1.5.78",
    "rescape-validate": ">=1.2.23"
  },
  "jest": {
    "modulePaths": [],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jest-environment-node",
    "testURL": "http://localhost",
    "transform": {},
    "setupFilesAfterEnv": [
      "<rootDir>/config/jestsetup.js"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ],
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": true,
              "regenerator": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
