{
  "name": "rescape-apollo",
  "version": "0.0.16",
  "description": "Apollo Helpers for Rescape Projects",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:calocan/rescape-apollo.git"
  },
  "author": "calocan <andy@statofplace.co>",
  "license": "MIT",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ENV=commonjs  node_modules/.bin/babel src --out-dir lib",
    "build:es": "BABEL_ENV=es NODE_ENV=es node $NODE_DEBUG_OPTION node_modules/.bin/rollup  -c",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development node_modules/.bin/rollup -c -out-dir dist",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production NODE_ENV=production rollup -c -out-dir dist",
    "build": "yarn run build:commonjs && yarn run build:es",
    "prepareX": "yarn run clean && yarn run lint && yarn test && yarn run build",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "yarn run docs:prepare && gitbook build -g reactjs/redux && cp logo/apple-touch-icon.png _book/gitbook/images/apple-touch-icon-precomposed-152.png && cp logo/favicon.ico _book/gitbook/images",
    "docs:watch": "yarn run docs:prepare && gitbook serve",
    "docs:publish": "yarn run docs:clean && yarn run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:reactjs/redux gh-pages --force",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn --new-version version patch && yarn publish",
    "publish-minor": "yarn --new-version version minor && yarn publish"
  },
  "dependencies": {
    "data.either": "^1.5.1",
    "data.maybe": "^1.2.2",
    "fetch": "^1.1.0",
    "folktale": "^2.1.0",
    "graphql-request": "^1.6.0",
    "http-https": "^1.0.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "ramda": "^0.25.0",
    "request": "^2.87.0",
    "rescape-helpers": "^1.1.39",
    "rescape-ramda": "^1.4.44"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "23.0.1",
    "babel-loader": "7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "6.26.0",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^5.2.0",
    "dotenv": "6.0.0",
    "eslint": "4.19.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "1.1.11",
    "fs-extra": "6.0.1",
    "jest": "23.1.0",
    "jest-cli": "^23.1.0",
    "jsdoc-babel": "^0.4.0",
    "longjohn": "^0.2.12",
    "promise": "8.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.7",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "url-loader": "^1.0.1"
  },
  "peerDependencies": {},
  "jest": {
    "modulePaths": [],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jestsetup.js",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "ie >= 11"
            ]
          },
          "exclude": [
            "transform-async-to-generator"
          ],
          "modules": false
        }
      ]
    ],
    "sourceMaps": true,
    "plugins": [
      "transform-object-rest-spread"
    ],
    "env": {
      "commonjs": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "env",
            {
              "node": "current"
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
